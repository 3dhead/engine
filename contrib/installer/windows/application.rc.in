#include "engine-config.h"
#endif

1 RT_MANIFEST DISCARDABLE "application.manifest"

#define HAS_ICON @HAS_ICON@
#define IDI_ICON 1001
#if HAS_ICON > 0
IDI_ICON ICON DISCARDABLE "@ICON@"
#endif

VS_VERSION_INFO VERSIONINFO
